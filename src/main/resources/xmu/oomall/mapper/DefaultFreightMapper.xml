<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="xmu.oomall.mapper.DefaultFreightMapper">
  <select id="selectByPrimaryKey" resultType="xmu.oomall.domain.DefaultFreightPo" parameterType="Integer">
